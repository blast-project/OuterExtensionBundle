{
    "name": "blast-project/outer-extension-bundle",
    "type": "symfony-bundle",
    "description": "Provides Outer Extension for Doctrine entities in Symfony bundles",
    "require": {
	"php": ">=5.6",
        "symfony/symfony": "3.2.*"
    },
    "require-dev": {
	"symfony/phpunit-bridge": "3.2.*"
    },
    "license": "LGPL-3.0",
    "keywords": [
        "symfony",
        "doctrine",
        "outer extension"
    ],
    "homepage": "https://github.com/blast-project/OuterExtensionBundle",
    "authors": [
        {
            "name": "Baptiste SIMON",
            "email": "baptiste.simon@libre-informatique.fr"
        },
        {
            "name": "Romain SANCHEZ",
            "email": "romain.sanchez@libre-informatique.fr"
        },
        {
            "name": "Marcos Bezerra de Menezes",
            "email": "marcos.bezerrademenezes@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Blast\\OuterExtensionBundle\\": "src/",
            "Blast\\OuterExtensionBundle\\Tests\\": "tests/"
        }
    }
}
